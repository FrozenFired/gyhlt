.navbar.navbar-expand-md.navbar-dark.bg-dark.fixed-top.noprint
	a.navbar-brand.btn.btn-success(href="/") <span class="oi oi-home"></span>
	if crUser
		-a;
		button.navbar-toggler(type="button" data-toggle="collapse" data-target="#navbarTogglerDemo02" aria-controls="navbarTogglerDemo02" aria-expanded="false" aria-label="Toggle navigation")
			<span class="navbar-toggler-icon"></span>

		.collapse.navbar-collapse#navbarTogglerDemo02
			ul.navbar-nav.mr-auto.mt-2.mt-xl-0
				if crUser.role == Conf.roleUser.brander.num || Conf.roleAdmin.includes(crUser.role)
					-a;
					li.nav-item.active
						a.nav-link(href="/bnBrands") 品牌
					li.nav-item.active
						a.nav-link(href="/bnPdfirs") 系列
					li.nav-item.active
						a.nav-link(href="/bnPdsecs") 产品
				if crUser.role == Conf.roleUser.quotation.num
					-a;
					li.nav-item.active
						a.nav-link(href="/qtQuts") 报价
				if crUser.role == Conf.roleUser.seller.num
					-a;
					li.nav-item.active
						a.nav-link(href="/slQuns") 询价
					li.nav-item.active
						a.nav-link(href="/slDins") 销售订单
				if crUser.role == Conf.roleUser.ordin.num
					-a;
					li.nav-item.active
						a.nav-link(href="/odDuts") 采购订单
				if crUser.role == Conf.roleUser.manager.num || Conf.roleAdmin.includes(crUser.role)
					-a;
					li.nav-item.active
						a.nav-link(href="/mgQuts") 报价管理
			ul.navbar-nav
				li
					a(href="/usFirm")
						span.navbar-brand [#{crUser.firm.code}]: 
					a(href="/user/"+crUser._id)
						span.navbar-brand
							if crUser.nome
								-a;
								strong #{crUser.nome}
							else
								strong #{crUser.code}
				li
					a.btn.btn-danger(href="/logout") <span class="oi oi-account-logout"></span>