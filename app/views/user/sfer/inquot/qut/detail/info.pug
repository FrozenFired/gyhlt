.border.border-dark.p-3
	.row.text-center
		.col-md-6
			h5.code 询价单号: #{qut.code}

			.time
				span 询价时间: 
				span #{moment(qut.qntcrtAt).format('YYYY年MM月DD日')}
			.qut 
				span 询价人:
				if qut.quner
					- quner = qut.quner;
					span #{quner.nome} [ #{quner.code} ]
				else
					span 数据丢失
			.cterNome
				span 客户: #{qut.cterNome}
		.col-md-6
			.quter
				span 报价人: 
				if qut.quter
					- quter = qut.quter;
					span #{quter.nome} [ #{quter.code} ]
				else
					span.text-warning 未分配
			.status.mt-3
				- let status = '';
				each sts in Conf.status
					- status = '';
					if sts.num == qut.status
						- status = sts.val;
						- break;
				span.p-2.text-danger 状态: #{status}
			.excel.text-right
				a(href="/slInquotExcel/"+qut._id) Excel <span class="oi oi-data-transfer-download"></span>
	.statusChange.text-center.row.mt-3
		if qut.status == Conf.status.quoting.num
			- a;
			if qut.compds.length == 0
				- a;
				.col-6
				.col-6
					span.p-2.border.border-danger 赞无询价商品
			else if qut.quterSt == Conf.status.init.num
				- a;
				.col-6
				.col-6
					span.quotingSpan.p-2.border.border-danger 请完成所有商品报价
					button.btn.btn-warning.changeStsAjax.quotingBtn(id="changeStsAjax-"+qut._id+"-done", type="button", data-url="/usInquotQuterStAjax", data-id=qut._id, data-stsf=qut.status, data-stst=Conf.status.done.num, style="display:none") [初步报价] 完成
			else
				.col-6
				.col-3
					span.p-2.border.border-success 已经完成
				.col-3
					button.btn.border-danger.text-danger.changeStsAjax.quotingBtn(id="changeStsAjax-"+qut._id+"-done", type="button", data-url="/usInquotQuterStAjax", data-id=qut._id, data-stsf=qut.status, data-stst=Conf.status.init.num, style="display:none") 返回未完成
		else if qut.status == Conf.status.pricing.num
			- a;
			.col-6
				button.mr-5.btn.border-danger.text-danger.changeStsAjax.quotingBtn(id="changeStsAjax-"+qut._id+"-pricing_quoting", type="button", data-url="/usInquotStatusAjax", data-id=qut._id, data-stsf=qut.status, data-stst=Conf.status.quoting.num) 返回处理
			.col-6
				button.ml-5.btn.btn-warning.changeStsAjax.quotingBtn(id="changeStsAjax-"+qut._id+"-pricing_confirm", type="button", data-url="/usInquotStatusAjax", data-id=qut._id, data-stsf=qut.status, data-stst=Conf.status.confirm.num) 完成
		else if qut.status == Conf.status.confirm.num
			-a;
			.col-6
				button.btn.border-danger.text-danger.changeStsAjax(id="changeStsAjax-"+qut._id+"-confirm_pricing", type="button", data-url="/usInquotStatusAjax", data-id=qut._id, data-stsf=qut.status, data-stst=Conf.status.pricing.num) 返回定价
			.col-6
		else if qut.status == Conf.status.pending.num
			-a;
			.col-6
				button.btn.border-danger.text-danger.changeStsAjax(id="changeStsAjax-"+qut._id+"-pending_confirm", type="button", data-url="/usInquotStatusAjax", data-id=qut._id, data-stsf=qut.status, data-stst=Conf.status.confirm.num) 返回确认状态
			.col-6
				a.btn.btn-success(href="/sfDinGen/"+qut._id) 去生成订单
		else if qut.status == Conf.status.unord.num
			-a;
			.col-6
				button.btn.border-danger.text-danger.changeStsAjax(id="changeStsAjax-"+qut._id+"-unord_pricing", type="button", data-url="/usInquotStatusAjax", data-id=qut._id, data-stsf=qut.status, data-stst=Conf.status.pricing.num) 重新定价
			.col-6
if qut.status == Conf.status.pricing.num
	-a;
	.row.py-4.text-right
		.col-4.col-md-9 同步供应商:
		.col-8.col-md-3
			if qut.strmup
				- strmup = qut.strmup;
				button.btn.btn-default.inquotUpClick#strmup-span #{strmup.nome}
			else
				button.btn.btn-default.inquotUpClick#strmup-span 未分配
			form#strmup-form(method="post", action="/sfQutUpd", enctype="multipart/form-data", style="display:none")
				input(type="hidden", name="obj[_id]", value=qut._id)
				.form-group.row.text-center
					.col-12
						select.inquotUpSel#strmup-ipt.form-control(name="obj[strmup]", data-style="btn-info")
							option(value='null') 未分配:
							each strmup in strmups
								-a;
								if !qut.strmup
									-a;
									option(value=strmup._id) #{strmup.nome}
								else if String(strmup._id) == String(qut.strmup._id)
									-a;
									option(value=strmup._id, selected="selected") #{strmup.nome}
								else
									option(value=strmup._id) #{strmup.nome}
				.form-group.row
					.col-6.text-left
						button.inquotCancelClick#strmup-cancel.btn.btn-secondary(type="button") 取消
					.col-6
						input.btn.btn-success(type="submit" value="提交")
script(src="/js/user/ajaxChange/status.js")
script(src="/js/dbs/compd/total.js")
script(src="/js/user/sfer/qut/detail.js")