.row.py-2.text-center.border
	.col-4.mt-3
		.quner
			span 询价人:
			if qut.quner
				- quner = qut.quner;
				span #{quner.nome} [ #{quner.code} ]
			else
				span 数据丢失
		.time
			span 询价时间: 
			span #{moment(qut.qntcrtAt).format('DD/MM/YYYY HH:mm:ss')}
		.excel.mt-3
			a.btn.btn-info.btn-block(href="/qtQutExcel/"+qut._id) Excel
	.col-4
		h3.code #{qut.code}
		.quter
			div 报价人:
			if qut.status == Conf.status.quoting.num
				-a;
				if qut.quter
					- quter = qut.quter;
					div.text-info.inquotUpClick#quter-span #{quter.nome} [ #{quter.code} ]
				else
					div.text-info.inquotUpClick#quter-span 未分配
				form#quter-form(method="post", action="/mgQutUpd", enctype="multipart/form-data", style="display:none")
					input(type="hidden", name="obj[_id]", value=qut._id)
					.form-group.row.px-5.text-center
						.col-12
							select.inquotUpSel#qutNote-ipt.form-control(name="obj[quter]", data-style="btn-info")
								option(value='null') 请选择报价员
								each quter in quters
									-a;
									if !qut.quter
										-a;
										option(value=quter._id) #{quter.nome}[#{quter.code}]
									else if String(quter._id) == String(qut.quter._id)
										-a;
										option(value=quter._id, selected="selected") #{quter.nome}[#{quter.code}]
									else
										option(value=quter._id) #{quter.nome}[#{quter.code}]
					.form-group.row
						.col-6
							button.inquotCancelClick#quter-cancel.btn.btn-secondary(type="button") 取消
						.col-6
							input.btn.btn-success(type="submit" value="提交")
			else
				if qut.quter
					- quter = qut.quter;
					div #{quter.nome} [ #{quter.code} ]
				else
					div.text-warning 未分配
	.col-4.mt-3
		if qut.status == Conf.status.done.num
			-a;
			h4 
				span 总价格: 
				span.text-info #{qut.price} €
		- let stsBg = 'bg-default';
		if qut.status == Conf.status.init.num
			- stsBg = 'bg-secondary';
		else if qut.status == Conf.status.done.num
			- stsBg = 'bg-info';
		else if qut.status == Conf.status.ord.num
			- stsBg = 'bg-success';
		else if qut.status == Conf.status.unord.num
			- stsBg = 'bg-danger';
		.status(class=stsBg)
			- let status = '';
			each sts in Conf.status
				- status = '';
				if sts.num == qut.status
					- status = sts.val;
					- break;
			span 状态: #{status}
		.statusChange.text-right.mt-4
			if qut.status == Conf.status.pricing.num
				- a;
				button.mr-5.btn.btn-warning.changeStsAjax.quotingBtn(id="changeStsAjax-"+qut._id+"-pricing_quoting", type="button", data-url="/usInquotStatusAjax", data-id=qut._id, data-stsf=qut.status, data-stst=Conf.status.quoting.num) 返回处理
				span &nbsp;
				button.ml-5.btn.btn-warning.changeStsAjax.quotingBtn(id="changeStsAjax-"+qut._id+"-pricing_done", type="button", data-url="/usInquotStatusAjax", data-id=qut._id, data-stsf=qut.status, data-stst=Conf.status.done.num) 完成
			else if qut.status == Conf.status.done.num
				-a;
				button.btn.btn-warning.changeStsAjax(id="changeStsAjax-"+qut._id+"-done_pricing", type="button", data-url="/usInquotStatusAjax", data-id=qut._id, data-stsf=qut.status, data-stst=Conf.status.pricing.num) 返回定价
			else if qut.status == Conf.status.unord.num
				-a;
				button.btn.btn-warning.changeStsAjax(id="changeStsAjax-"+qut._id+"-unord_pricing", type="button", data-url="/usInquotStatusAjax", data-id=qut._id, data-stsf=qut.status, data-stst=Conf.status.pricing.num) 重新定价
if qut.status == Conf.status.pricing.num
	-a;
	.row.py-4.text-right
		.col-4.col-lg-8 同步供应商
		.col-8.col-lg-4
			if qut.strmup
				- strmup = qut.strmup;
				div.text-info.inquotUpClick#strmup-span #{strmup.nome}
			else
				div.text-info.inquotUpClick#strmup-span 未分配
			form#strmup-form(method="post", action="/mgQutUpd", enctype="multipart/form-data", style="display:none")
				input(type="hidden", name="obj[_id]", value=qut._id)
				.form-group.row.text-center
					.col-12
						select.inquotUpSel#strmup-ipt.form-control(name="obj[strmup]", data-style="btn-info")
							option(value='null') 未分配:
							each strmup in strmups
								-a;
								if !qut.strmup
									-a;
									option(value=strmup._id) #{strmup.nome}
								else if String(strmup._id) == String(qut.strmup._id)
									-a;
									option(value=strmup._id, selected="selected") #{strmup.nome}
								else
									option(value=strmup._id) #{strmup.nome}
				.form-group.row
					.col-6.text-left
						button.inquotCancelClick#strmup-cancel.btn.btn-secondary(type="button") 取消
					.col-6
						input.btn.btn-success(type="submit" value="提交")
script(src="/js/user/ajaxChange/status.js")
script(src="/js/user/qter/qut/detail/basicUp.js")