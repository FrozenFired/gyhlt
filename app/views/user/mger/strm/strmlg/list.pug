extends ../../../aalayout/layout

block content
	.container.mt-3
		.title
			.row
				.col-9.text-muted.mt-2: h5
					a(href="/") Home
					span &nbsp; | &nbsp;
					span 物流公司列表
				.col-3.text-right
					a.btn.btn-warning(href="/mgStrmlgAdd") +添加物流公司
		.row.mt-5.text-center
			each strmlg in strmlgs
				-a;
				.col-md-3.card.p-3.mt-3
					a.btn.btn-info(href="/mgStrmlg/"+strmlg._id) #{strmlg.code}
					h5.mt-3 #{strmlg.nome}
